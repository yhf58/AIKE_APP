                        </div><!-- service-item -->
                    </div>
                </div><!-- row -->
            </div><!-- service-items -->
        </div><!-- container -->
    </section>
    <!-- services section end -->
    @* 热门进度 *@
    <!-- skills section start -->
    <section id="skills" class="skills padding-120">
        <div class="container">
            <div class="row">
                <div class="col-md-6 col-sm-6 col-xs-12">
                    <div class="section-header">
                        <h1 style="font-weight:900">热门项目筹集进度</h1>
