@{
    ViewBag.Title = "Home Page";
}
@using AIKE_Model;

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Home</title>
    <!-- Bootstrap -->
    <link href="../Lib/Home/Index/assets/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Font-Awesome -->
    <link href="../Lib/Home/Index/assets/css/font-awesome.min.css" rel="stylesheet" />

    <!-- Lightcase -->
    <link href="../Lib/Home/Index/assets/css/lightcase.css" rel="stylesheet" />

    <!-- Swiper -->
    <link href="../Lib/Home/Index/assets/css/swiper.min.css" rel="stylesheet" />

    <!-- Animate -->
    <link href="../Lib/Home/Index/assets/css/animate.css" rel="stylesheet" />

    <!-- Style -->
    <link href="../Lib/Home/Index/assets/css/style.css" rel="stylesheet" />

    <!-- Responsive -->
    <link href="../Lib/Home/Index/assets/css/responsive.css" rel="stylesheet" />



    <link href="~/Common/bootstrap/css/bootstrap-grid.min.css" rel="stylesheet" />

    <link href="~/Common/Index/style.css" rel="stylesheet" />

    <style>
        .t_num i {
            width: 33px;
            height: 47px;
            display: inline-block;
            background: url('../../Images/number1.png')no-repeat;
            background-position: 0 0;
        }
    </style>
</head>

<body data-spy="scroll">
    <!-- Preloader start here -->
    <div class="preloader">
        <div class="loader">
            <div class="ld-child"></div>
            <div class="ld-child ld-child2"></div>
        </div>
    </div>
    @* 导航栏 *@
    <header>
        <nav class="main-menu menu-fixed">
            <div class="container">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a href="index.html" style="font-size:30px;color:#ffffff;text-decoration:content;line-height: 90px;">艾客众筹</a>
                </div>
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav navbar-right">
                        <li><a class="page-scroll" href="#banner">艾客</a></li>
                        <li><a class="page-scroll" href="#portfolio">项目</a></li>
                        <li><a class="page-scroll" href="#skills">合作</a></li>
                        <li><a class="page-scroll" href="#blog">新闻</a></li>
                        <li><a class="page-scroll" href="#contact">关于</a></li>
                        @if (Session["person"] == null)
                        {<li style="width:180px;height:90px;border:0px solid red;position:absolute;right:20px;top:0px;"><a class="page-scroll" href="../Home/Login" style="color:red;">亲，请登录...</a></li>}
                        @if (Session["person"] != null) {
                         Userinfo u = (Userinfo)Session["person"];
                        <li style="width:180px;height:90px;border:0px solid red;position:absolute;right:20px;top:0px;">
                            <a class="page-scroll" href="../Home/Person?user=@u.AccountNumber">
                                <img src="~/Lib/Home/Login/img/头像.png" style="width:30px;height:30px;" />@u.AccountNumber,你好！
                            </a></li> }
                    </ul>

                </div><!-- /.navbar-collapse -->

            </div><!-- /.container -->
        </nav>
    </header>

    @* 头部 *@
    <section id="banner" class="banner">
        <div class="overlay">
            <div class="banner-content">
                <h3>全球专业实体空间投资平台</h3>
                <p>认真对待每一个梦想</p>
                <a href="#about" class="default-button page-scroll">成为投资者</a>
                <!--项目筹集进度-->
                <div class="container">

                    <div class="row" style="padding-top:50px;">
                        <div class="col-xs-6 col-sm-3"
                             style="background-color: #000000;
         box-shadow: inset 1px -1px 1px #444, inset -1px 1px 1px #444;background:rgba(255,255,255,0.3);">
                            <p>
                                <span class="t_num t_num1"></span>
                            <p>
                                参与人<br>
                                用户的信任是我们最大的财富
                            </p>
                            </p>
                        </div>
                        <div class="col-xs-6 col-sm-3"
                             style="background-color: #000000;
         box-shadow: inset 1px -1px 1px #444, inset -1px 1px 1px #444;background:rgba(255,255,255,0.3);">
                            <p>
                                <span class="t_num" id="t_num2"></span>
                            <p>
                                投资项目<br>
                                遍布全球1488个目的地资源海量
                            </p>
                            </p>
                        </div>

                        <div class="clearfix visible-xs"></div>

                        <div class="col-xs-6 col-sm-3"
                             style="background-color: #000000;
         box-shadow:inset 1px -1px 1px #444, inset -1px 1px 1px #444;background:rgba(255,255,255,0.3);">
                            <p>
                                <span class="t_num" id="t_num3"></span>
                            <p>
                                成交金额<br>
                                总投资金额达163.84亿
                            </p>
                            </p>
                        </div>
                        <div class="col-xs-6 col-sm-3"
                             style="background-color: #000000;box-shadow:
         inset 1px -1px 1px #444, inset -1px 1px 1px #444;background:rgba(255,255,255,0.3);">
                            <p>
                                <span class="t_num" id="t_num4"></span>
                            <p>
                                成立至今<br>
                                不忘初心，方得始终
                            </p>
                            </p>
                        </div>
                    </div>
                </div>
            </div><!-- banner content -->
        </div><!-- overlay -->
    </section>

    @*众筹项目*@
    <!-- Portfolio section start -->
    <section id="portfolio" class="portfolio bg-color">
        <div class="container">
            <div class="section-header text-center">
                <h1 style="font-weight:900">众筹项目</h1>
                <p><i></i></p>
            </div>
            @* 项目类型 *@
            <ul class="portfolio-menu">
                <li class="active" data-filter="*">所有类型</li>
                <li data-filter=".design"><a href="../Home/Index?typename=1"> 科技</a></li>
                <li data-filter=".photography"><a href="../Home/Index?typename=2">食品</a></li>
                <li data-filter=".illustration"><a href="../Home/Index?typename=3">设计</a></li>
                <li data-filter=".vector"><a href="../Home/Index?typename=4">文学</a></li>
                <li data-filter=".vector"><a href="../Home/Index?typename=5">娱乐</a></li>
                <li data-filter=".vector"><a href="../Home/Index?typename=6">公益</a></li>
            </ul>
            @* 项目列表 *@
            <div class="portfolio_items">
                @foreach (Project itme in (List<Project>)ViewData["Read"])
                {
                    <div class="col-md-4 col-sm-6">
                        <div class="box">
                            <div class="box-img">
                                <img style="width:250px;height:300px;" src="~/Images/@itme.ProjectCover" />
                            </div>
                            <div class="box-content">
                                <h4 class="title">@itme.ProjectName</h4>
                                <p class="description">@itme.ProjectIntroduction</p>
                                <a href="../Home/Details?ProjectID=@itme.ProjectID" class="btn btn-success">详 情</a>
                            </div>

                        </div>

                    </div>
                }

            </div><!-- portfolio items -->
            @* 分页 *@
            <div style="width:30%;height:5%; margin:0px auto;line-height:300%;">
                <nav aria-label="Page navigation">
                    <ul class="pagination pagination-lg" style="margin-top:20%;">
                        <li>
                            <a href="#" aria-label="Previous">
                                <span aria-hidden="true">&laquo;</span>
                            </a>
                        </li>
                        <li><a href="#">1</a></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li><a href="#">4</a></li>
                        <li><a href="#">5</a></li>
                        <li>
                            <a href="#" aria-label="Next">
                                <span aria-hidden="true">&raquo;</span>
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>

        </div><!-- container -->
    </section>

    <!-- Portfolio section end -->
    <!-- Client section start -->
    @* 项目分类 *@
    <!-- Client section end -->
    <!-- services section start -->
    <section id="services" class="services padding-120 bg-color">
