                            <div class="progress">
                                <div class="progress-bar  wow slideInLeft" role="progressbar" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100" style="width: 80%;"><span>80%</span></div>
                            </div>
                        </div><!-- bar-item -->
                    </div><!-- skill-bars -->
                </div>
            </div><!-- row -->
        </div><!-- container -->
    </section>
    <!-- skills section end -->
